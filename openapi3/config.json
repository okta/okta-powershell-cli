{
    "packageName" : "Okta.PowerShell",
    "isApiKey" : false,
    "isBasicBearer" : true,
    "isBasicBasic": false,
    "apiNamePrefix": "Okta",
    "templateDir": "./codegen-templates",
    "outputDir" : "../",
    "inputSpec" : "./management.yaml", 
    "packageVersion" : "0.1.0",
    "packageDescription" : "Official PowerShell CLI for the Okta API",
    "packageTitle" : "Official PowerShell for the Okta API",
    "packageCompany" : "Okta, Inc.",
    "packageAuthors" : "Okta, Inc.",
    "packageTags" : "okta api authentication identity management",
    "packageCopyright" : "Okta, Inc.",
    "gitHost": "github.com",
    "gitUserId" : "okta",
    "gitRepoId" : "okta-powershell-cli", 
    "globalProperties": {
        "apiTests": false,
        "modelTests": false
    },
    "authMethods": [{
        "isApiKey" : false,
        "isBasicBearer" : true,
        "isBasicBasic": false
    }],
    "files": {
        "okta_authenticationApi.mustache":{
            "destinationFilename": "OktaAuthenticationApi.ps1",
            "templateType": "SupportingFiles",
            "folder": "/src/Okta.PowerShell/Api"
        },
        "url_helper.mustache":{
            "destinationFilename": "UrlHelper.ps1",
            "templateType": "SupportingFiles",
            "folder": "/src/Okta.PowerShell/Client"
        },
        "okta_retryApi.mustache":{
            "destinationFilename": "OktaRetryApi.ps1",
            "templateType": "SupportingFiles",
            "folder": "/src/Okta.PowerShell/Client"
        },
        "api_README.mustache":{
            "destinationFilename":"API_README.md",
            "templateType": "SupportingFiles",
            "folder": "/"
        }
    }
}